 type Query {
    "show Test"
    shows(titleFilter: String): [Show]
}

 type Mutation {
     "show Test add"
     addShows(show: showInfo): Boolean
 }

type Show {
    title: String
    releaseYear: Int
}

 input showInfo {
     title: String
     releaseYear: Int
 }

 schema {
     query: Query
     mutation: Mutation
 }
